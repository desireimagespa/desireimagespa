            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background-color": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            ".underline": {
              "padding-top": ".5rem",
              ".line": {
                "width": "5rem",
                "height": "1px",
                "display": "inline-block",
                "background-color": "@underlineColor",
                "transition": "width .3s"
              }
            },
            "& when (@showTitle) and (@showSubtitle)": {
              ".mbr-section-subtitle": {
                "padding-top": "1rem"
              }
            },
            ".title-wrap:hover": {
              ".line": {
                "width": "6rem"
              }
            },
            ".title-wrap": {
              "padding-bottom": "3rem"
            },
            ".card": {
              "&:hover": {
                ".line": {
                  "width": "6rem"
                }
              },
              "& when (@showIcons)": {
                ".iconfont-wrapper": {
                  "display": "inline-block",
                  "width": "~\"@{iconsSize}rem\"",
                  "height": "~\"@{iconsSize}rem\""
                },
                ".card-img": {
                  "padding-bottom": "1rem",
                  ".mbr-iconfont": {
                    "color": "@iconsColor",
                    "transition": "color .5s",
                    "font-size": "~\"@{iconsSize}rem\""
                  }
                }
              },
              ".mbr-card-title": {
                "margin": "0"
              },
              ".underline": {
                "padding": ".6rem 0 1rem 0"
              },
              "p": {
                "margin": "0"
              }
            },
            "& when not (@showUnderline)": {
              ".mbr-card-title": {
                "padding-bottom": "1rem"
              }
            },
            "& when (@showSocial)": {
              ".social-wrap": {
                "margin-top": "2rem",
                "padding-top": "2rem"
              },
              ".iconfont-wrapper-social": {
                "display": "inline-block",
                "width": "calc(~\"@{socialSize}rem + 1rem\")",
                "height": "calc(~\"@{socialSize}rem + 1rem\")",
                "margin": "0 .5rem"
              },
              ".amp-iconfont-social": {
                "padding": ".5rem",
                "border": "1px solid @socialColor",
                "border-radius": "50%",
                "color": "@socialColor",
                "transition": "all .4s",
                "width": "calc(~\"@{socialSize}rem + 1rem\")",
                "height": "calc(~\"@{socialSize}rem + 1rem\")",
                "font-size": "~\"@{socialSize}rem\"",
                "&:hover": {
